# OpenRouter.ai API Configuration
# Copy this file to .env and replace with your actual API key
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenRouter.ai API Base URL (optional, defaults to https://openrouter.ai/api/v1)
VITE_OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Model name to use with OpenRouter.ai
VITE_MODEL_NAME=mistralai/mistral-small-3.2-24b-instruct:free

# Supabase
VITE_SUPABASE_URL=
VITE_SUPABASE_KEY=
VITE_PROJECT_NAME=